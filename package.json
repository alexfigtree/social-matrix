{
  "name": "social-matrix",
  "description": "social matrix project",
  "version": "1.0.1",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "eject": "react-scripts eject",
    "client-install": "npm install --prefix client",
    "build": "cd client && npm run-scripts build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "cd client && npm start",
    "server-prod": "node src/App.js",
    "server": "nodemon index.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm run start\"",
    "start:dev": "concurrently --kill-others  \"npm run client\" \"npm run server\" ",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.3",
    "nodemon": "^2.0.7",
    "react-d3-radar": "^1.0.0-rc6",
    "requirejs": "^2.3.6"
  }
}
